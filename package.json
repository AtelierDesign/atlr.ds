{
    "name": "atlr.ds",
    "packageManager": "npm@8.12.1",
    "version": "0.0.1",
    "description": "Component library for shared UI @AtelierDesign",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.es.js",
    "files": [
        "dist"
    ],
    "workspaces": [
        "packages/*",
        "app/*"
    ],
    "scripts": {
        "ds:build": "rollup -c",
        "ds:dev": "rollup -c -w",
        "ds:release": "npm run ds:build && np",
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "np": "npm publish"
    },
    "dependencies": {
        "@radix-ui/colors": "^0.1.8",
        "@radix-ui/react-accordion": "^1.0.1",
        "@radix-ui/react-aspect-ratio": "^1.0.1",
        "@radix-ui/react-checkbox": "^1.0.0",
        "@radix-ui/react-dialog": "^1.0.2",
        "@radix-ui/react-hover-card": "^1.0.2",
        "@radix-ui/react-icons": "^1.1.1",
        "@radix-ui/react-label": "^2.0.0",
        "@radix-ui/react-separator": "^1.0.0",
        "@radix-ui/react-switch": "^1.0.1",
        "@radix-ui/react-tabs": "^1.0.0",
        "@radix-ui/react-toast": "^1.0.0",
        "@stitches/react": "^1.2.8",
        "cmdk": "^0.1.19",
        "inter-ui": "^3.19.3",
        "lodash.merge": "^4.6.2",
        "next": "^13.0.3",
        "next-themes": "^0.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-fast-marquee": "^1.3.5"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^22.0.2",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@types/lodash": "^4.14.184",
        "@types/lodash.merge": "^4.6.7",
        "@types/node": "^18.7.14",
        "@types/react": "^18.0.18",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "eslint": "^8.23.0",
        "eslint-config-next": "^12.2.5",
        "prettier": "^2.7.1",
        "prop-types": "^15.7.0",
        "rollup": "^2.79.0",
        "rollup-plugin-typescript2": "^0.33.0",
        "tslib": "^2.4.0",
        "turbo": "^1.6.2",
        "typescript": "4.3.5"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AtelierDesign/atlr.ds.git"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "prettier": {
        "semi": true,
        "singleQuote": true,
        "arrowParens": "always",
        "tabWidth": 2,
        "printWidth": 120,
        "trailingComma": "none"
    },
    "author": "chvndler <chan@ady.world>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AtelierDesign/atelier.ds/issues"
    },
    "homepage": "https://github.com/AtelierDesign/atelier.ds#readme"
}
